void FromList():{}
{
    FromTable()[(<COMMA>FromTable())+]
}
void FromTable():{}
{
    TableName() [[<AS>] <IDENTIFIER>]
    /* TODO:连接查询和嵌套查询 */
    | LOOKAHEAD(2)<LEFTPARENTHESES> SelectStmt() <RIGHTPARENTHESES>
}
